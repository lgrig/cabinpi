{
  "formatters": {"f": {"format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"}},
  "handlers": {"debug": {"class": "logging.FileHandler", "filename": "debug.log", "formatter": "f", "level": "logging.DEBUG"},
              "info": {"class": "logging.FileHandler", "filename": "info.log", "formatter": "f", "level": "logging.INFO"},
              "error": {"class": "logging.FileHandler", "filename": "info.log", "formatter": "f", "level": "logging.ERROR"}
            },
  "loggers": {"default": {"level": "INFO", "handlers": ["info","error"], "propogate": "no"}}.
  "root": {"handlers": "h", "level": "logging.DEBUG"}
}
